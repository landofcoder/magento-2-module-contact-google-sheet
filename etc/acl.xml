<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Lof_Contact2GoogleSheet::element" title="Contact 2 Google Sheet">
                    <resource id="Lof_Contact2GoogleSheet::contact" title="Send Contact" sortOrder="10">
                        <resource id="Lof_Contact2GoogleSheet::contact_save" title="Contact Save" sortOrder="10" />
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Lof_Contact2GoogleSheet::config" title="Import Contact Form to Google Sheet" translate="title" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
